<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/activity_main2"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:background="#b4ebf7"
    tools:context="siet.com.tell_info.Main2Activity">

    <ImageButton
        android:layout_width="180dp"
        android:layout_height="150dp"
        app:srcCompat="@drawable/cabinfo"
        android:id="@+id/imageButton8"
        android:contentDescription=""
        tools:ignore="ContentDescription"
        android:layout_alignParentBottom="true"
        android:layout_alignParentStart="true" />

    <ImageButton
        app:srcCompat="@drawable/newhotel"
        android:id="@+id/imageButton2"
        android:layout_height="160dp"
        android:layout_width="180dp"
        android:layout_toEndOf="@+id/imageButton8"
        android:layout_alignTop="@+id/imageButton8" />

    <ImageButton
        android:layout_height="180dp"
        app:srcCompat="@drawable/hospitalinfo"
        android:id="@+id/imageButton13"
        tools:ignore="ContentDescription"
        android:layout_width="340dp"
        android:layout_centerVertical="true"
        android:layout_alignParentStart="true" />

    <ImageButton
        android:layout_width="180dp"
        android:layout_height="150dp"
        app:srcCompat="@drawable/collegeinfo"
        android:id="@+id/imageButton20"
        tools:ignore="ContentDescription"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_toEndOf="@+id/imageButton15" />

    <ImageButton
        android:layout_width="180dp"
        android:layout_height="150dp"
        app:srcCompat="@drawable/schoolinfo"
        android:id="@+id/imageButton15"
        tools:ignore="ContentDescription"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true" />

</RelativeLayout>
